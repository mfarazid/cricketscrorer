.col-xs-12.col-sm-9.page-content
  .omb_login
    %heading.center
      %h1 Welcome to MCCS!    
      %h2 Login to start!
    %hr   
    .row.omb_row-sm-offset-3.omb_socialButtons
      .col-xs-4.col-sm-2
        %a.btn.btn-lg.btn-block.omb_btn-facebook{:href => '/auth/facebook', id: 'sign_in' }
          %i.fa.fa-facebook
        %p.center Facebook  
      .col-xs-4.col-sm-2
        %a.btn.btn-lg.btn-block.omb_btn-twitter{:href => '/auth/twitter', id: 'sign_in'}
          %i.fa.fa-twitter
        %p.center Twitter   
      .col-xs-4.col-sm-2
        %a.btn.btn-lg.btn-block.omb_btn-google{:href => '/auth/google_oauth2', id: 'sign_in' }
          %i.fa.fa-google-plus
        %p.center Google+   
= render 'shared/sidebar'           